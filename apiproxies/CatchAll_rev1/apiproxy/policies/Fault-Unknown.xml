<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="Fault-Unknown">
    <DisplayName>Fault Unknown</DisplayName>
    <FaultRules/>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
          <Payload contentType="application/json">\{"error" : "Unknown Resource"} </Payload>
            <StatusCode>404</StatusCode>
            <ReasonPhrase>Resource Not Found</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>