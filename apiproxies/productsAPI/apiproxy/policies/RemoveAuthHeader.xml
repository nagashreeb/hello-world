<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage enabled="true" continueOnError="false" async="false" name="RemoveAuthHeader">
    <DisplayName>RemoveAuthHeader</DisplayName>
    <FaultRules/>
    <Properties/>
    <Remove>
        <Headers>
            <Header name="Authorization"/>
            <Header name="Accept-Encoding"/>
            <Header name="Accept-Charset"/>
            <Header name="User-Agent"/>
        </Headers>
        <QueryParams>
            <QueryParam name="access_token"/>
        </QueryParams>
    </Remove>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
</AssignMessage>
