<ExtractVariables name="ExtractProduct">
	<Source clearPayload="false">getproduct_response</Source>
    <VariablePrefix>myretail</VariablePrefix>
     <JSONPayload>
        <Variable name="productname" type="string">
            <JSONPath>$.entities[0].productname</JSONPath>
        </Variable>
        <Variable name="productimage" type="string">
            <JSONPath>$.entities[0].productimage</JSONPath>
        </Variable>
    </JSONPayload>
</ExtractVariables>
